{
  "headers": [{
    "source": "/(.*)",
    "headers": [{
        "key": "Cache-Control",
        "value": "public, s-maxage=86400, max-age=60, stale-while-revalidate"
      },
      {
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      }
    ]
  }],
  "buildCommand": "pnpm turbo build",
  "ignoreCommand": "pnpm dlx turbo-ignore"
}